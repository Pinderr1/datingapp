{
  "name": "rn-rc-datingapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --passWithNoTests --watchAll=false",
    "build": "echo \"Dry build complete\"",
    "lint": "node scripts/run-eslint.js"
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native|@react-navigation|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|@expo/vector-icons|expo-font|expo-asset|expo-constants|expo-router|@expo/.*|firebase/.*))"
    ],
    "moduleNameMapper": {
      "\\.(png|jpg|jpeg|gif|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@miblanchard/react-native-slider": "^2.6.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-spring/native": "^9.5.5",
    "boardgame.io": "^0.50.2",
    "expo": "54.0.21",
    "expo-blur": "*",
    "expo-constants": "*",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "*",
    "expo-linking": "*",
    "expo-location": "~19.0.7",
    "expo-router": "~6.0.13",
    "expo-splash-screen": "*",
    "expo-status-bar": "*",
    "expo-symbols": "*",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "*",
    "firebase": "^12.2.1",
    "lottie-react-native": "~7.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-collapsible-toolbar": "^0.1.2",
    "react-native-deck-swiper": "^2.0.19",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-gesture-handler": "*",
    "react-native-otp-entry": "^1.8.2",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "*",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "^0.5.1",
    "rn-range-slider": "^2.2.2"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^3.0.1",
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.13",
    "typescript": "^5.3.3"
  },
  "private": true
}
